{
	"name": "@bracketed/typescript-config",
	"version": "0.1.4",
	"description": "Just some extensions for tsconfig.",
	"author": "@bracketed",
	"main": "tsconfig.json",
	"contributors": [
		{"name": "ninjaninja140", "url": "https://github.com/ninjaninja140"}
	],
	"exports": {
		".": {
			"import": "./tsconfig.json",
			"require": "./tsconfig.json"
		},
		"./base-config": {
			"import": "./base-config.json",
			"require": "./base-config.json"
		},
		"./without-decorators": {
			"import": "./without-decorators.json",
			"require": "./without-decorators.json"
		},
		"./extra-strict": {
			"import": "./extra-strict.json",
			"require": "./extra-strict.json"
		},
		"./extra-strict-without-decorators": {
			"import": "./extra-strict-without-decorators.json",
			"require": "./extra-strict-without-decorators.json"
		}
	},
	"sideEffects": false,
	"homepage": "https://bracketed.co.uk",
	"dependencies": {
		"typescript": "^4.6.3"
	},
	"files": [
		"tsconfig.json",
		"extra-strict*.json",
		"base-config.json",
		"*decorators*.json"
	],
	"engines": {
		"node": ">=v16.0.0",
		"npm": ">=8.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bracketed/Bracketed-Packages.git",
		"directory": "packages/ts-config"
	},
	"bugs": {
		"url": "https://github.com/bracketed/Bracketed-Packages/issues"
	},
	"keywords": [
		"bracketed",
		"typescript",
		"ts",
		"yarn",
		"typescript config"
	],
	"publishConfig": {
		"access": "public"
	}
}
